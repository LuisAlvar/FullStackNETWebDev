{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/src/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/public/favicon.ico",
          "/src/index.html",
          "/src/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "public",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/public/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ]
}
